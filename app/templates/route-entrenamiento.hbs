

<!--==========================================
                   ENTRENAMIENTO
===========================================-->
<section id="experience" class="section">
    <div class="container">
        <!-- SECTION TITLE -->
        <div class="section-title">
            <h4 class="text-uppercase text-center"><i class="title-icon fa fa-suitcase"></i>Entrenamiento</h4>
        </div>

        <div id="timeline-experience">

            {{#each webapidata as |item|}} 
                <!-- FIRST TIMELINE -->
                <div class="timeline-block">
                    <!-- DOT -->
                    <div class="timeline-dot"><h6>Letra</h6></div>
                    <!-- TIMELINE CONTENT -->
                    <div class="card timeline-content">
                        <div class="card-content">
                            <!-- TIMELINE TITLE -->
                            <h6 class="timeline-title">{{item.Nombre}}</h6>
                            <!-- TIMELINE TITLE INFO -->
                            <div class="timeline-info">                                
                                <h6>
                                    Institucion:
                                    <small>{{item.Institucion}}</small>
                                </h6>
                                <h6>
                                    Nro. Horas:
                                    <small>{{item.NroHoras}}</small>
                                </h6>
                                <h6>
                                    Fecha Entrega:
                                    <small>{{item.FechaEntrega}}</small>
                                </h6>
                                <h6>
                                    Fecha Fin:
                                    <small>{{item.FechaFinVigencia}}</small>
                                </h6>
                            </div>
                            <!-- TIMELINE PARAGRAPH -->
                            <p>
                                {{item.Descripcion}}
                            </p>
                            <!-- BUTTON TRIGGER MODAL -->
                            <a href="#" class="modal-dot" data-toggle="modal" data-target="#myModal-4">
                                <i class="fa fa-ellipsis-h" aria-hidden="true"></i>
                            </a>
                        </div>
                    </div>
                </div>
            {{/each}}

        </div>
    </div>
</section>

